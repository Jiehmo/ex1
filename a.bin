#include <stdio.h>

int main() {
    FILE* fp;
    int a[] = {0, 1, 2};
    char b[] = "ABC";
    float c[] = {1.1, 1.2, 1.3};

    // 輸出整數陣列
    for (int i = 0; i < 3; i++) {
        printf("%d ", a[i]);
    }
    printf("\n");

    // 輸出字元陣列
    for (int i = 0; i < 3; i++) {
        printf("%c ", b[i]);
    }
    printf("\n");

    // 輸出浮點數陣列
    for (int i = 0; i < 3; i++) {
        printf("%.5f ", c[i]);
    }
    printf("\n");

    return 0;
}
